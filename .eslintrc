    {
        "extends": ["airbnb","prettier", "prettier/react"],
        "plugins": ["prettier"],
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 6,
            "ecmaFeaturea": {
                "jsx": true
            }
        },
        "env": {
            "browser": true,
            "jest": true,
            "es6": true
        },
        "rules": {
            "react/jsx-filename-extension": 0,
            "camelcase": [
                "error",
                {
                    "properties": "always"
                }
            ],
            "jsx-a11y/anchor-is-valid": [
                "error",
                {
                    "components": [
                        "Link"
                    ],
                    "specialLink": [
                        "to"
                    ]
                }
            ],
            "react/forbid-prop-types": 0,
            "no-undef": 0,
            "no-multiple-empty-lines": [
                2,
                {
                    "max": 2,
                    "maxBOF": 0,
                    "maxEOF": 0
                }
            ],
            "max-lines": [
                "error",
                150
            ],
            "max-statements-per-line": [
                "error",
                {
                    "max": 1
                }
            ]
        },
    }